{
  "manifest_version": 2,
  "name": "Metabar killer",
  "version": "1.0",
  "description": "Removes the top menu from hackernoon, because it's too big.",
  "content_scripts": [
    {
      "matches": [
        "*://*.hackernoon.com/*",
        "*://*.medium.com/*"
      ],
      "js": [
        "metabar-killer.js"
      ]
    }
  ]
}